{
  "name": "odyssey",
  "private": true,
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1"
  },
  "workspaces": [
    "apps/*",
    "services/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "bun run --cwd apps/web dev",
    "dev:worker": "bun run --cwd services/worker dev",
    "build:web": "bun run --cwd apps/web build",
    "check": "bun run --cwd apps/web lint && bun test",
    "test": "bun test"
  }
}
